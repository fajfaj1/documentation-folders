{
    "manifest_version": 3,
    "name": "CC documentation folders",
    "version": "1.6.9",  
    
    "description": "A simple extension allowing you to fold sections in CC docs",
    "icons": {
      "128": "icon.png"
    },

    "author": "fajfaj#6941", 
    "content_scripts": [
        {
          "matches": ["https://doc.ccommandbot.com/*"],
          "css": ["styles.css"],
          "js": ["script.js"]
        }
      ]
}